interface=wlan0     # Use your wireless interface
driver=nl80211
ssid=YourSSID
hw_mode=g
channel=6
ieee80211n=1

# Hotspot 2.0 (Passpoint) configuration
ieee80211u=1
interworking=1
hs20=1
hs20_oper_friendly_name=YourHotspotName
hs20_wan_metrics=1

# RADIUS server configuration
auth_server_addr=127.0.0.1
auth_server_port=1812
auth_server_shared_secret=YourSecret




